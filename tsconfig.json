{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components": ["components/index"],
      "@constants/*": ["constants/*"],
      "@hooks": ["hooks/index"],
      "@localization": ["localization/index"],
      "@navigation": ["navigation/index"],
      "@navigation/stacks": ["navigation/stacks/index"],
      "@screens": ["screens/index"],
      "@store": ["store/index"],
      "@types": ["types/index"],
      "@utils": ["utils/index"],
      "src/*": ["src/*"],
    }
  }
}
